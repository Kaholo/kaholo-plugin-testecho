{
  "name": "testecho",
  "viewName": "Kaholo Testecho Plugin",
  "type": "executer",
  "main": "app.js",
  "execProgram": "node",
  "exposeRoute": false,
  "imgUrl": "logo.png",
  "version": "1.0.0",
  "description": "This plugin is used only to test Kaholo functionality.",
  "category": [
    "Testing"
  ],
  "keywords": [
    "testecho",
    "one",
    "TWO",
    "tHREe",
    "OneTwoThree",
    "keyword with spaces",
    "keyword-with-dashes"
  ],
  "auth": {
    "authId": "kaholo-plugin-testecho",
    "params": [
      {
        "name": "authString01",
        "viewName": "Auth String 01 - unconfigured",
        "type": "string",
        "description": "Leave this unconfigured",
        "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
      },
      {
        "name": "authString02",
        "viewName": "Auth String 02 - with config.json default value",
        "type": "string",
        "description": "This should default to \"defaulted\" without being configured",
        "default": "defaulted",
        "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
      },
      {
        "name": "authString03",
        "viewName": "Auth String 03 - user configured, required",
        "type": "string",
        "description": "Configure this to something as a user",
        "required": true,
        "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
      }
    ]
  },
  "settings": [
    {
      "name": "settingString01",
      "viewName": "Setting String 01 - unconfigured",
      "type": "string",
      "description": "Leave this unconfigured",
      "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
    },
    {
      "name": "settingString02",
      "viewName": "Setting String 02 - with config.json default value",
      "type": "string",
      "description": "This should default to \"defaulted\" without being configured",
      "default": "defaulted",
      "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
    },
    {
      "name": "settingString03",
      "viewName": "Setting String 03 - user configured, required",
      "type": "string",
      "description": "Configure this to something as a user",
      "required": true,
      "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
    }
  ],
  "methods": [
    {
      "name": "testStringsBasic",
      "viewName": "Test String Params, Settings, and Accounts",
      "params": [
        {
          "name": "testRunName",
          "viewName": "testRunName for Final Result",
          "type": "string",
          "required": false,
          "placeholder": "Run-00001",
          "description": "Any string to identify this test run",
          "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
        },
        {
          "name": "paramString01",
          "viewName": "Param String 01 - unconfigured",
          "type": "string",
          "description": "Leave this unconfigured",
          "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
        },
        {
          "name": "paramString02",
          "viewName": "Param String 02 - with config.json default value",
          "type": "string",
          "description": "This should default to \"defaulted\" without being configured",
          "default": "defaulted",
          "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
        },
        {
          "name": "paramString03",
          "viewName": "Param String 03 - user configured, required",
          "type": "string",
          "description": "Configure this to something as a user",
          "required": true,
          "learnUrl": "https://kaholo.io/docs/kaholo-getting-started/"
        }    
      ]
    }
  ]
}